pipeline {
    agent any 
    stages {
        stage('Test') {
        	withMaven(maven: 'M3') {
	            echo 'Test' 
	            sh "mvn clean compile test"
            }
        }
        stage('Build') {
            steps {
                echo 'Generate image' 
                sh "mvn clean install -Dmaven.test.skip=true"
            }
        }
        stage('Publish') {
        	steps {
        		echo 'Publis to Web Server'
        	}
        }
    }
}